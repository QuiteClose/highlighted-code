{
  "name": "highlighted-code",
  "version": "0.3.0",
  "description": "A textarea builtin extend to automatically provide code highlights based on one of the languages available via highlight.js",
  "main": "./cjs/index.js",
  "scripts": {
    "build": "npm run cjs && npm run rollup:es && npm run rollup:index",
    "cjs": "ascjs --no-default esm cjs",
    "rollup:es": "rollup --config rollup/es.config.js",
    "rollup:index": "rollup --config rollup/index.config.js"
  },
  "keywords": [
    "textarea",
    "highlight"
  ],
  "author": "Andrea Giammarchi",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "ascjs": "^5.0.1",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "module": "./esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "default": "./cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "unpkg": "min.js",
  "dependencies": {
    "highlight.js": "^11.5.0"
  }
}
